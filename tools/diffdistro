#!/bin/bash
d=`pwd`
mkdir -p cache/tumbleweed cache/slowroll
( cd cache/tumbleweed && $d/tools/getprimary https://download.opensuse.org/tumbleweed/repo/oss/ )
( cd cache/slowroll && $d/tools/getprimary http://stage3.opensuse.org:17080/repositories/SUSE%3A/ALP%3A/Experimental%3A/Slowroll/base/repo/oss/ )

